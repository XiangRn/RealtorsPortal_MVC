        $(function () {

             $.ajax({
                type: "GET",
                url: "/Home/GetModeId",
                datatype: "Json",
                success: function (data) {
                    $('#mode1').empty();
                    $("#mode1").append("<option value='null'>--Select Mode--</option>");
                    $.each(data, function (index, value) {


                        $('#mode1').append('<option value="' + value.ModeId + '">' + value.ModeName + '</option>');
                    });
                }
            });
            $.ajax({
                type: "GET",
                url: "/Home/GetCity",
                datatype: "Json",
                success: function (data) {
                    $('#dropdownCity').empty();
                    $("#dropdownCity").append("<option value='null'>--Select City--</option>");
                    $.each(data, function (index, value) {


                        $('#dropdownCity').append('<option value="' + value.CityId + '">' + value.CityName + '</option>');
                    });
                }
            });

            $('#dropdownCity').change(function () {

                $('#dropdownDistrict').empty();
                $('#dropdownWard').empty();
                $('#dropdownStreet').empty();

                $.ajax({
                    type: "POST",
                    url: "/Home/GetDistrict",
                    datatype: "Json",
                    data: { cityId: $('#dropdownCity').val() },
                    success: function (data) {

                        $("#dropdownDistrict").append("<option value='null'>--Select District--</option>");
                        $("#dropdownWard").append("<option value='null'>--Select Ward--</option>");
                        $("#dropdownStreet").append("<option value='null'>--Select Street--</option>");
                        $.each(data, function (index, value) {


                            $('#dropdownDistrict').append('<option value="' + value.DistrictId + '">' + value.DistrictName + '</option>');
                        });
                    }
                });
            });


            $('#dropdownDistrict').change(function () {

                $('#dropdownWard').empty();
                $('#dropdownStreet').empty();

                $.ajax({
                    type: "POST",
                    url: "/Home/GetWard",
                    datatype: "Json",
                    data: { districtId: $('#dropdownDistrict').val() },
                    success: function (data) {

                        $("#dropdownWard").append("<option value='null'>--Select Ward--</option>");
                        $("#dropdownStreet").append("<option value='null'>--Select Street--</option>");
                        $.each(data, function (index, value) {

                            $('#dropdownWard').append('<option value="' + value.WardId + '">' + value.WardName + '</option>');
                        });
                    }
                });
            });

            $('#dropdownWard').change(function () {

                $('#dropdownStreet').empty();

                $.ajax({
                    type: "POST",
                    url: "/Home/GetStreet",
                    datatype: "Json",
                    data: { wardId: $('#dropdownWard').val() },
                    success: function (data) {

                        $("#dropdownStreet").append("<option value='null'>--Select Street--</option>");
                        $.each(data, function (index, value) {

                            $('#dropdownStreet').append('<option value="' + value.StreetId + '">' + value.StreetName + '</option>');
                        });
                    }
                });
            });



        });


